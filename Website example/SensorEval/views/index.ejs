<!DOCTYPE html>
<html>
  <head>
    <%- include('partials/header') %>
  </head>
  <body>
     <%- include('partials/navbar') %> 

    <div class="container">
      <div class="row">
        <div class="col-sm-6">
          <h3 class="text-center">Latest Session Uploads</h3>
        </div>
        <div class="col-sm-6">
          <h3 class="text-center">Patients newly registered</h3>
        </div>
      </div>
      <div class="row">
        <div class="col-sm-6">
          <div class="table-responsive">
            <table class="table table-hover">
              <% latestUploads.forEach(function(patient) { %>
                <tr>
                  <td><a href="/patient/?PatientID=<%= patient.PatientID %>"><%= patient.Name %></a></td>
                  <td><a href="/session/?SessionID=<%= patient.SessionID %>"><%= patient.DateTimeUploaded %></a></td>
                </tr>
              <% }); %>
            </table>
          </div> 
        </div>
        <div class="col-sm-6">
          <div class="table-responsive">
            <table class="table table-hover">
              <% newlyRegistered.forEach(function(patient) { %>
                <tr>
                  <td><a href="/patient/?PatientID=<%= patient.PatientID %>"><%= patient.Name %></a></td>
                  <td><%= patient.DateRegistered %></td>
                </tr>
              <% }); %>
            </table>
          </div> 
        </div>
      </div> 
    </div>

  </body>
